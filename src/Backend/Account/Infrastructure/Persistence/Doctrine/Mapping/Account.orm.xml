<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Kishlin\Backend\Account\Domain\Account" table="accounts">
        <id name="id" type="account_id" column="id" length="36" />

        <field name="username" type="account_username" column="username" />

        <field name="email" type="account_email" column="email" />

        <field name="password" type="account_password" column="password" />

        <field name="salt" type="account_salt" column="salt" />

        <field name="isActive" type="account_is_active" column="is_active" />
    </entity>

</doctrine-mapping>

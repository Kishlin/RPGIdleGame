<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Kishlin\Backend\RPGIdleGame\Character\Domain\Character" table="characters">
        <id name="id" type="character_id" column="id" length="36" />

        <field name="name" type="character_name" column="name" />

        <field name="owner" type="character_owner" column="owner" length="36" />

        <field name="skillPoint" type="character_skill_point" column="skill_points" />

        <field name="health" type="character_health" column="health" />

        <field name="attack" type="character_attack" column="attack" />

        <field name="defense" type="character_defense" column="defense" />

        <field name="magik" type="character_magik" column="magik" />

        <field name="rank" type="character_rank" column="rank" />

        <field name="activeStatus" type="character_active_status" column="is_active" />
</entity>

</doctrine-mapping>
